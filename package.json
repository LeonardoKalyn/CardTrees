{
  "name": "tree-cards",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "axios": "^0.20.0",
    "http-proxy-middleware": "^1.0.5",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-jss": "^10.4.0",
    "react-scripts": "3.4.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test-no-watch": "test --watchAll=false",
    "eject": "react-scripts eject",
    "lint": "eslint ./src --no-ignore --max-warnings 0"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "husky": "^4.3.0",
    "nock": "^13.0.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn run lint && yarn run test-no-watch",
      "pre-push": "yarn run lint && yarn run test-no-watch"
    }
  }
}
